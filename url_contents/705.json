eUrl: \"/TDClient/37/uic/WebServices/JSON/TagLookupService.asmx/GetItems\",\r\n      searchTextBoxSelector: '#SiteSearch-text-860429069c424f5ea5b070a1d431c53f',\r\n      searchComponentFilterButtonSelector: '#SiteSearch-filter-860429069c424f5ea5b070a1d431c53f',\r\n      searchButtonSelector: '#SiteSearch-button-860429069c424f5ea5b070a1d431c53f',\r\n      searchFilterDropdownSelector: '#SiteSearch-dropdown-860429069c424f5ea5b070a1d431c53f',\r\n      showKnowledgeBaseOption: true,\r\n      showServicesOption: true,\r\n      showForumsOption: false,\r\n      autoFocusSearch: false\r\n    });\r\n  });\r\n</script>\r\n              </div>\r\n\r\n              \r\n              <div title=\"Sign In\" class=\"pull-right gutter-top-sm\">\r\n                <a href=\"/TDClient/37/uic/Login.aspx?ReturnUrl=%2fTDClient%2f37%2fuic%2fHome%2f\">Sign In\r\n                </a>\r\n              </div>\r\n              \r\n            </div>\r\n\r\n          </div>\r\n\r\n        </div>\r\n\r\n        <nav id=\"ctl00_mainNav\" class=\"navbar navbar-default themed tdbar\" aria-label=\"Primary\">\r\n          <div id=\"navContainer\" class=\"container\">\r\n\r\n            <div class=\"navbar-header\">\r\n              <button type=\"button\" class=\"pull-left navbar-toggle collapsed themed tdbar-button-anchored tdbar-toggle gutter-left\" data-toggle=\"collapse\" data-target=\".navbar-collapse\">\r\n                <span class=\"tdbar-button-anchored icon-bar\"></span>\r\n                <span class=\"tdbar-button-anchored icon-bar\"></span>\r\n                <span class=\"tdbar-button-anchored icon-bar\"></span>\r\n                <span class=\"sr-only\">Show Applications Menu</span>\r\n              </button>\r\n              <a class=\"navbar-brand hidden-sm hidden-md hidden-lg themed tdbar-button-anchored truncate-be\" href=\"#\">\r\n\r\n                UIC Help Center\r\n              </a>\r\n            </div>\r\n\r\n            <div id=\"td-navbar-collapse\" class=\"collapse na