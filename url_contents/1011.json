rc=g;m.parentNode.insertBefore(a,m)\r\n  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');\r\n\r\n  ga('create', 'UA-70525865-1', 'auto');\r\n  ga('send', 'pageview');\r\n\r\n</script><script>\r\n  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){\r\n  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),\r\n  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)\r\n  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');\r\n\r\n  ga('create', 'UA-70558901-1', 'auto');\r\n  ga('send', 'pageview');\r\n\r\n</script>\t\t<div>\r\n\r\n\t\t\t\r\n\t\t\t\n<a href=\"https://alumni.jmls.uic.edu/\">\n\t<div style=\"width:100%; border-bottom:16px solid #D50032; background: #D50032;\">\n\t\t<div class=\"wrapper-top\">\n\t\t\t<img src=\"/img/alumni_jmls_logo_giving.png\" style=\"max-width:100%; height:auto;\" alt=\"\"/>\t\t</div>\n\t</div>\n</a>\n<div style=\"display:none\">\n\t<a id=\"refresh\">refresh</a>\n</div>\n<div id=\"block-content\">\n\n<p></p>\n\n<div style=\"margin: 0px auto; max-width: 700px;\">\n\t<h1 style=\"margin-bottom:3px;\">Give a Gift</h1>\n\n\t<div style=\"overflow:hidden; float:left;\">\n\t\t<a href=\"https://uif.uillinois.edu/matching-gifts\" class=\"show1\" target=\"_blank\">Did you know you can double the value of your gift?</a>\n\n\t</div>\n</div>\n\n<div class=\"wrapper\" style=\"margin-top:30px;\">\n\t<section id=\"content\">\n\n\t\t<section id=\"module-search\" >\n\t\t\t<div class=\"searchWidget\">\n\t\t\t\t<input type=\"text\" name=\"search\" id=\"search-input\" placeholder=\"Option 1: Search for Funds and Programs by Proper Name\" />\n            </div>\n\t\t\t<div class=\"divider\"></div>\n        </section>\n\n\t\t<section id=\"list_fund\" style='display:none;'>\n\t\t\t <article style=\"display: block;\" class=\"widget\" id=\"priorities-results\">\n\t\t\t\t<header class=\"title\">\n\t\t\t\t\t<img src=\"/img/magnify.png\" class=\"titleIcon\" alt=\"\"/>\t\t\t\t\t<h3>Fund/Program List</h3>\n\t\t\t\t\t<div class=\"num\">\n\t\t\t\t\t\t<a class=\"Num\"><span id=\"list_fund-count\"></span></a>\n\t\t\t\t\t</div>\n\t\t\t\t</header>\n\t\t\t\t<div class=\"content\" id=\"resultsFund\" style=\"overflow:scroll; max-height:250px;\">\n\t\t\t\t</div>\n\t\t\t\t<footer class=\"action\">\n\t\t\t\t   <div class=\"num\">\n\t\t\t\t\t   <a class=\"redNum\" id=\"closeFund\" title=\"Close List\">Close List</a>\n\t\t\t\t\t</div>\n\t\t\t\t</footer>\n\t\t\t </article>\n\t\t\t <div class=\"divider\"></div>\n\t\t</section>\n\n\t\t<section id=\"module-priorities\" role=\"priorities\" >\n             <div class=\"priorityWidget\">\n                <div action=\"/\" class=\"priorities\">\n                   <fieldset>\n                      <select id=\"priorities-input\" name=\"priorities-input\" >\n                         <option value=\"\" style=\"color:gray !important;\">Option 2: Select Fund or Program</option>\n                         <option style='color:black !important;' value='3'>Annual Fund</option><option style='color:black !important;' value='1'>Centers for Excellence</option><option style='color:black !important;' value='5'>Community Legal Clinics</option><option style='color:black !important;' value='4'>Honors Programs</option><option style='color:black !important;' value='7'>International Programs</option><option style='color:black !important;' value='9'>Other Funds</option><option style='color:black !important;' value='2'>Scholarships & Awards</option><option style='color:black !important;' value='6'>Student Organizations</option>                      </select>\n                   </fieldset>\n                </div>\n             </div>\n          </section>\n\n\t\t  <!-- /#module-priorities -->\n          <section id=\"list_category\" style='display:none;'>\n             <article style=\"display: block;\" class=\"widget\" id=\"priorities-results\">\n                <header class=\"title\">\n                   \t<img src=\"/img/magnify.png\" class=\"titleIcon\" alt=\"\"/>                   \t<h3>Fund/Program List</h3>\n                   \t<div class=\"num\">\n\t\t\t\t\t   <a class=\"Num\"><span id=\"list_category-count\"></span></a>\n\t\t\t\t\t</div>\n                </header>\n                <div class=\"content\" id=\"resultsCategory\" style=\"overflow:scroll; max-height:250px;\">\n                </div>\n                <footer class=\"action\">\n                   <div class=\"num\"><a class=\"redNum\" id=\"closeCategory\" title=\"Close List\">Close List</a></div>\n                </footer>\n             </article>\n          </section>\n\n          <!-- /#module-priorities -->\n          <div class=\"divider\"></div>\n          <form action=\"https://www.uif.uillinois.edu/gifts/PaymentPageTransfer.aspx\" name=\"Form1\" id=\"Form1IndexForm\" method=\"post\" accept-charset=\"utf-8\"><div style=\"display:none;\"><input type=\"hidden\" name=\"_method\" value=\"POST\"/></div>\t\t  <input type=\"hidden\" name=\"TRACKING_CODE\" value=\"21051\">\n          <input type=\"hidden\" name=\"SKIN_ID\" value=\"chicagoannualgiving\"/ >\n\n          <h2>Your Gift Cart</h2>\n          <article id=\"list_gifting\" class=\"widget\">\n             \t<header class=\"title\">\n                \t<img src=\"/img/donation-list.png\" class=\"titleIcon\" alt=\"\"/>                \t<h3>Donation List</h3>\n                \t<div class=\"num\">\n\t\t\t\t\t\t<a class=\"redNum\" id=\"\" style=\"cursor:default !important;\"><input id='listCartCount' style=\"border: none; background: transparent; width: 10px; color: #fff; font-size: 11px\" class=\"form-text donation-amount\" value=\"0\" type=\"text\">Funds</a>\n\t\t\t\t\t</div>\n             \t</header>\n             \t<div id=\"cart-list\" class=\"content\">\n                \t<div class=\"gift-fund-name\" style=\"padding: 15px;\">Your gift cart is empty. Please add funds.</div>\n             \t</div>\n             \t<footer class=\"action footer clearfix\">\n                \t<h4 class=\"float-left\">Total:</h4>\n                \t<h4 class=\"float-right\">$<span id=\"TotalAmount\"><input id='listCartTotal' style=\"border: none; background: transparent; text-align: left; width: 65px; font-size: 14px\" class=\"form-text donation-amount\" value=\"0\" type=\"text\" readonly></span></h4>\n             \t</footer>\n          </article>\n          <div style=\"display: block;\" id=\"CheckoutButton\" >\n             <a id=\"proceedCheckout\" title=\"Proceed to Checkout\" class=\"wContentButton\">Proceed to Checkout</a>\n          </div>\n          <div style=\"display:none\"><div class=\"submit\"><input type=\"submit\" value=\"Add\"/></div></form></div>\n       </section>\n    </div>\n</div>\n\n<script>\nvar fundsUrl = '/donation_funds/fund.json';\nvar fundByCategoryUrl = '/donation_funds/fund_category.json';\n$(function() {\n  $(\"#cart-list\").html('<div class=\"gift-fund-name\" style=\"padding: 15px;\">Your gift cart is empty. Please add funds.</div>');\n    $(\"#listCartCount\").val('0');\n    $(\"#listCartTotal\").val(0);\n    $('#priorities-input').css('color','gray');\n   $('#priorities-input').change(function() {\n      var current = $('#priorities-input').val();\n      if (current != '') {\n          $('#priorities-input').css('color','black');\n      } else {\n          $('#priorities-input').css('color','gray');\n      }\n   });\n });\n</script>\n<script type=\"text/javascript\">\n\n$(document).ready(function(){\n\n    $('.show1').click(function(){\n\t$('#pop1').simplePopup();\n    });\n\n    $('.show2').click(function(){\n\t$('#pop2').simplePopup();\n    });\n\n});\n\n</script>\n\n<div class=\"refund\" style=\"width:100%;!important; background-color:#eee;\">\n\t<div style=\"width:100%; margin:0 auto; max-width:700px;\">\n\n\t\t<div style=\"width:100%; float:right;\">\n\t\t\t<h4>Questions or concerns?</h4>\n\t\t\tContact us at:<br />\n\t\t\t<strong>UIC John Marshall Law School<br />\n\t\t\tOffice of Alumni Relations &amp; Development</strong><br />\n\t\t\t300 S. State Street<br />\n\t\t\tChicago, Illinois 60604<br />\n\t\t\t<strong><a href=\"mailto:law-alumni@uic.edu\">law-alumni@uic.edu</a></strong><br />\n\t\t\t<strong style=\"float:right;\"><a href=\"//www.vpaa.uillinois.edu/resources/web_privacy\">Privacy Statement</a></strong>\n\t\t\t(312) 360-2663\n\t\t</div>\n\n</div>\n\t\t</div>\r\n</body>\r\n</html>\r\n"}